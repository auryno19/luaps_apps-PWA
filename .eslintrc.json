{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-underscore-dangle": "off",
        "space-before-function-paren": 0,
        "arrow-body-style" :0,
        "prefer-destructuring" :0,
        "no-extra-semi": 0,
        "no-alert": "off",
        "no-console":0
    }
}
